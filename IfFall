using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.SceneManagement;

public class IfFall : MonoBehaviour
{
    public Rigidbody rb;

    void FixedUpdate()
    {
        if (rb.position.y < -20f)
        {
            SceneManager.LoadScene(0);
        }
    }
}
